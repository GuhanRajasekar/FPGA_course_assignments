#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x555f31ca4fa0 .scope module, "mux_tb_juice" "mux_tb_juice" 2 1;
 .timescale 0 0;
v0x555f31cbaa10_0 .var "data", 3 0;
v0x555f31cbab00_0 .var "sel", 1 0;
v0x555f31cbabd0_0 .net "y", 0 0, L_0x555f31cbacd0;  1 drivers
S_0x555f31ca5130 .scope module, "m1" "mux4" 2 5, 3 1 0, S_0x555f31ca4fa0;
 .timescale 0 0;
    .port_info 0 /INPUT 4 "data";
    .port_info 1 /INPUT 2 "sel";
    .port_info 2 /OUTPUT 1 "y";
v0x555f31c6dba0_0 .net "data", 3 0, v0x555f31cbaa10_0;  1 drivers
v0x555f31cba810_0 .net "sel", 1 0, v0x555f31cbab00_0;  1 drivers
v0x555f31cba8f0_0 .net "y", 0 0, L_0x555f31cbacd0;  alias, 1 drivers
L_0x555f31cbacd0 .part/v v0x555f31cbaa10_0, v0x555f31cbab00_0, 1;
    .scope S_0x555f31ca4fa0;
T_0 ;
    %vpi_call 2 8 "$monitor", $time, " data=%b, sel=%b, y=%b ", v0x555f31cbaa10_0, v0x555f31cbab00_0, v0x555f31cbabd0_0 {0 0 0};
    %delay 5, 0;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x555f31cbaa10_0, 0, 4;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x555f31cbab00_0, 0, 2;
    %delay 5, 0;
    %pushi/vec4 1, 0, 4;
    %store/vec4 v0x555f31cbaa10_0, 0, 4;
    %pushi/vec4 1, 0, 2;
    %store/vec4 v0x555f31cbab00_0, 0, 2;
    %delay 5, 0;
    %pushi/vec4 2, 0, 4;
    %store/vec4 v0x555f31cbaa10_0, 0, 4;
    %pushi/vec4 2, 0, 2;
    %store/vec4 v0x555f31cbab00_0, 0, 2;
    %delay 5, 0;
    %pushi/vec4 3, 0, 4;
    %store/vec4 v0x555f31cbaa10_0, 0, 4;
    %pushi/vec4 3, 0, 2;
    %store/vec4 v0x555f31cbab00_0, 0, 2;
    %delay 5, 0;
    %pushi/vec4 4, 0, 4;
    %store/vec4 v0x555f31cbaa10_0, 0, 4;
    %pushi/vec4 2, 0, 2;
    %store/vec4 v0x555f31cbab00_0, 0, 2;
    %delay 5, 0;
    %pushi/vec4 5, 0, 4;
    %store/vec4 v0x555f31cbaa10_0, 0, 4;
    %pushi/vec4 1, 0, 2;
    %store/vec4 v0x555f31cbab00_0, 0, 2;
    %delay 5, 0;
    %pushi/vec4 6, 0, 4;
    %store/vec4 v0x555f31cbaa10_0, 0, 4;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x555f31cbab00_0, 0, 2;
    %delay 5, 0;
    %pushi/vec4 7, 0, 4;
    %store/vec4 v0x555f31cbaa10_0, 0, 4;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x555f31cbab00_0, 0, 2;
    %delay 5, 0;
    %pushi/vec4 8, 0, 4;
    %store/vec4 v0x555f31cbaa10_0, 0, 4;
    %pushi/vec4 3, 0, 2;
    %store/vec4 v0x555f31cbab00_0, 0, 2;
    %delay 5, 0;
    %pushi/vec4 9, 0, 4;
    %store/vec4 v0x555f31cbaa10_0, 0, 4;
    %pushi/vec4 2, 0, 2;
    %store/vec4 v0x555f31cbab00_0, 0, 2;
    %delay 5, 0;
    %pushi/vec4 10, 0, 4;
    %store/vec4 v0x555f31cbaa10_0, 0, 4;
    %pushi/vec4 1, 0, 2;
    %store/vec4 v0x555f31cbab00_0, 0, 2;
    %delay 5, 0;
    %pushi/vec4 11, 0, 4;
    %store/vec4 v0x555f31cbaa10_0, 0, 4;
    %pushi/vec4 2, 0, 2;
    %store/vec4 v0x555f31cbab00_0, 0, 2;
    %delay 5, 0;
    %vpi_call 2 21 "$finish" {0 0 0};
    %end;
    .thread T_0;
